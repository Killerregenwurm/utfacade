<?xml version="1.0" encoding="UTF-8"?>

<!--
    Document   : ExampleResponse.xml
    Created on : May 7, 2007, 10:42 AM
    Author     : Daniel Pustka, Peter Keitler
    Description: This files shows a simple example of a UTQL response as could be generated by the Ubitrack server. It belongs to
    the UTQL requests as shown in ExampleAppQuery.xml, ExampleGraphSpec.xml and PatternSpec.xml.
-->

<UTQLResponse name="exampleResponse" xmlns='http://ar.in.tum.de/ubitrack/utql'
              xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
              xsi:schemaLocation='http://ar.in.tum.de/ubitrack/utql http://ar.in.tum.de/files/ubitrack/utql/utql_types.xsd'>
                  
    <Pattern name="Art" id="267235412">
        <Output>
            <Node name="Art" id="Art">
                <Attribute name="ArtPort" value="5000"/>
            </Node>
        </Output>
    </Pattern>
    
    <Pattern name="Hmd" id="02295875">
        <Input>
            <Node name="Art" id="Art">
                <Attribute name="ArtPort" value="5000"/>
            </Node>
        </Input>
        <Output>
            <Node name="ArtHmd" id="ArtHmd"/>
            <Edge name="ArtToArtHmd" source="Art" destination="ArtHmd"> 
                <Attribute name="type" value="6DOF"/>
                <Attribute name="ArtBodyId" value="3"/>
            </Edge>
        </Output>
        <DataflowConfiguration>
            <UbitrackLib class="ArtTracker"/>
        </DataflowConfiguration>
    </Pattern>
    
    <Pattern name="Sheep" id="520981785">
        <Input>
            <Node name="Art" id="Art">
                <Attribute name="ArtPort" value="5000"/>
            </Node>
        </Input>
        <Output>
            <Node name="ArtSheep" id="ArtSheep"/>
            <Edge name="ArtToArtSheep" source="Art" destination="ArtSheep">
                <Attribute name="type" value="6DOF"/>
                <Attribute name="ArtBodyId" value="7"/>
            </Edge>
        </Output>
        <DataflowConfiguration>
            <UbitrackLib class="ArtTracker"/>
        </DataflowConfiguration>
    </Pattern>
    
    <Pattern name="Multiplication" id="93759875">
        <Input>
            <Node name="NodeA" id="ArtHmd"/>
            <Node name="NodeB" id="Art">
                <Attribute name="ArtPort" value="5000"/>
            </Node>
            <Node name="NodeC" id="ArtSheep"/>
            <Edge name="AB" source="NodeA" destination="NodeB" pattern-ref="16987234" edge-ref="BA">
                <Attribute name="type" value="6DOF"/>
            </Edge>
            <Edge name="BC" source="NodeB" destination="NodeC" pattern-ref="520981785" edge-ref="ArtToArtSheep">
                <Attribute name="type" value="6DOF"/>
            </Edge>
        </Input>
        <Output>
            <Edge name="AC" source="NodeA" destination="NodeC">
                <Attribute name="type" value="6DOF"/>
            </Edge>
        </Output>
        <DataflowConfiguration>
            <UbitrackLib class="PosePushPullMultiplication"/>
        </DataflowConfiguration>
    </Pattern>
    
    <Pattern name="Inversion" id="16987234">
        <Input>
            <Node name="NodeA" id="Art">
                <Attribute name="ArtPort" value="5000"/>
            </Node>
            <Node name="NodeB" id="ArtHmd"/>
            <Edge name="AB" source="NodeA" destination="NodeB" pattern-ref="02295875" edge-ref="ArtToArtHmd">
                <Attribute name="type" value="6DOF"/>
            </Edge>
        </Input>
        <Output>
            <Edge name="BA" source="NodeB" destination="NodeA">
                <Attribute name="type" value="6DOF"/>
            </Edge>
        </Output>
        <DataflowConfiguration>
            <UbitrackLib class="PosePushInversion"/>
        </DataflowConfiguration>
    </Pattern>
    
    <Pattern name="SheepQuery" id="2347545">
        <Input>
            <Node name="Hmd" id="ArtHmd"/>
            <Node name="Sheep" id="ArtSheep"/>
            <Edge name="TheEdge" source="Hmd" destination="Sheep" pattern-ref="93759875" edge-ref="AC">
                <Attribute name="type" value="6DOF"/>
            </Edge>
        </Input>
        <DataflowConfiguration>
            <UbitrackLib class="ApplicationPushSink"/>
        </DataflowConfiguration>
    </Pattern>
    
</UTQLResponse>
